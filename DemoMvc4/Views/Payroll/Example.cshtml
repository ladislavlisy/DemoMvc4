                                        </div>
                                        <div class="span2 text-right">@DemoMvc4.Models.PayrollExample.ValueFromDetails(detail)
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>
    <br />
    <table class="table table-bordered">
        <tbody>
            <tr>
                <td>
                    <div class="span11">
                        <div class="row">
                            <div class="span5 offset1">
                                @foreach (var detail in Model.ExportPart2L)
                                {
                                    <div class="row">
                                        <div class="span3 text-left">
