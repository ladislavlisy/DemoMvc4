                                            <img src="~/Content/images/@DemoMvc4.Models.PayrollExample.ImageFromDetails(detail)" class="image-circle image-ikona" />
                                            @DemoMvc4.Models.PayrollExample.TitleFromDetails(detail)
                                        </div>
                                        <div class="span2 text-right">@DemoMvc4.Models.PayrollExample.ValueFromDetails(detail)
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>
    <br />
    <div class="row">
        <div class="span12">
            <div class="row">
                <div class="span10 offset1 text-center">
                    Paycheck details by Mzdy Hravě - Online payroll
                    agenda
                </div>
            </div>
        </div>
    </div>
    <p />
</div>
